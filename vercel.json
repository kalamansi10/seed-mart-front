{
  "rewrites": [
    {
      "source": "/api/:endpoint*",
      "destination": "https://api.seedmart.biz/api/v1/:endpoint*"
    },
    {
      "source": "/users/:endpoint*",
      "destination": "https://api.seedmart.biz/users/:endpoint*"
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ]
}